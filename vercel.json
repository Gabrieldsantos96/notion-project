{
    "version":2,
    "public":true,
    "builds":[
        {
            "src":"src/main/server.ts",
            "use": "@vercel/node",
            "entrypoints":[
                {
                    "src":"src/main/server.ts",
                    "dest":"api/server.js"
                }
            ]
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}